{
  "name": "sheet2scene",
  "version": "1.0.1",
  "private": true,
  "packageManager": "npm@10.8.0",
  "type": "module",
  "engines": {
    "node": ">=20.10.0"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=web",
    "build": "npm run build -w engine && npm run build -w web",
    "start": "npm run start --workspace=web",
    "aaa": "node scripts/aaa-engine.js all",
    "aaa:check": "node scripts/aaa-engine.js check",
    "aaa:fix": "node scripts/aaa-engine.js fix",
    "aaa:patch": "node scripts/aaa-engine.js patch",
    "aaa:triage": "node scripts/aaa-engine.js triage",
    "aaa:ci": "node scripts/aaa-engine.js ci",
    "aaa:task": "node scripts/aaa-engine.js one"
  },
  "devDependencies": {
    "lint-staged": "^15.2.0",
    "tailwindcss": "^3.4.13",
    "turbo": "^2.1.0",
    "typescript": "^5.4.0"
  },
  "dependencies": {
    "@supabase/ssr": "^0.7.0",
    "zod": "^4.1.13"
  }
}
